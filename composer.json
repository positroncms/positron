{
  "name": "positron/positron",
  "type": "library",
  "description": "Positron CMS Core",
  "keywords": [
    "positron",
    "cms",
    "laravel",
    "core"
  ],
  "homepage": "http://www.positroncms.com/",
  "support": {
    "issues": "https://github.com/positroncms/positron/issues",
    "source": "https://github.com/positroncms/positron"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Kalanj Djordje",
      "email": "kalanjdjordje@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.3.*",
    "dimsav/laravel-translatable": "~5.0|~6.0",
    "symfony/translation": "~2.6|~3.0",
    "yajra/laravel-datatables-oracle": "~6.0",
    "nwidart/laravel-modules": "^1.14",
    "orchestra/asset": "^3.3",
    "maatwebsite/laravel-sidebar": "^2.1",
    "arcanedev/log-viewer": "~4.0",
    "propaganistas/laravel-translatable-bootforms": "~1.3",
    "lavary/laravel-menu": "^1.6",
    "barryvdh/laravel-debugbar": "^2.3",
    "yajra/laravel-datatables-oracle": "~6.0",
    "arrilot/laravel-widgets": "^3.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "autoload": {
    "files": [
      "src/Foundation/helpers.php",
      "src/Support/helpers.php"
    ],
    "psr-4": {
      "Positron\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
